{
  "$schema": "https://playground.wordpress.net/blueprint-schema.json",
  "meta": {
    "title": "Step 1: Install Plugins",
    "description": "Installing and activating plugins for Cozy Community site",
    "author": "Cozy Community"
  },
  "preferredVersions": {
    "php": "8.2",
    "wp": "latest"
  },
  "login": {
    "username": "admin",
    "password": "password"
  },
  "steps": [
    {
      "step": "resetData"
    },
    {
      "step": "importWordPressFiles",
      "wordPressFilesZip": {
        "resource": "url",
        "url": "https://www.dropbox.com/scl/fi/6a3yk6mzu0j8wo6fbo5hh/wp-content.zip?rlkey=0215egl6bl0mg1l2ajlwi6cxi&st=7uy0kv5p&dl=1"
      }
    },
    {
      "step": "installTheme",
      "themeData": {
        "resource": "wordpress.org/themes",
        "slug": "twentytwentyfour"
      },
      "options": {
        "activate": false
      }
    },
    {
      "step": "installPlugin",
      "pluginData": {
        "resource": "wordpress.org/plugins",
        "slug": "bbpress"
      }
    },
    {
      "step": "installPlugin",
      "pluginData": {
        "resource": "url",
        "url": "https://github.com/a8cteam51/community-gallery-tags/archive/refs/heads/main.zip"
      },
      "options": {
        "activate": true
      }
    },
    {
      "step": "installPlugin",
      "pluginData": {
        "resource": "wordpress.org/plugins",
        "slug": "buddypress"
      }
    },
    {
      "step": "wp-cli",
      "command": "wp rewrite flush"
    }
    
  ]
}
